<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cz.sample">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.cz.sample.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.cz.sample.ui.adapter.HeaderAdapterActivity" />
        <activity android:name="com.cz.sample.ui.adapter.DynamicAdapterActivity" />
        <activity android:name="com.cz.sample.ui.recyclerview.PullToRefreshActivity" />
        <activity android:name="com.cz.sample.ui.recyclerview.GridPullToRefreshActivity" />
        <activity android:name="com.cz.sample.ui.recyclerview.PullToRefreshExpandActivity" />
        <activity android:name="com.cz.sample.ui.adapter.TreeAdapterViewActivity" />
        <activity android:name="com.cz.sample.ui.drag.LinearDragActivity" />
        <activity android:name="com.cz.sample.ui.drag.GridDragActivity" />
        <activity android:name="com.cz.sample.ui.drag.DynamicDragActivity" />
        <activity android:name="com.cz.sample.ui.drag.CustomDragActivity" />
        <activity android:name="com.cz.sample.ui.adapter.SelectAdapterActivity" />

        <activity android:name="com.cz.sample.ui.sticky.Sticky1SampleActivity"/>
        <activity android:name="com.cz.sample.ui.sticky.Sticky2SampleActivity"/>
        <activity android:name="com.cz.sample.ui.sticky.Sticky3SampleActivity"/>
        <activity android:name="com.cz.sample.ui.sticky.Sticky4SampleActivity"/>
        <activity android:name="com.cz.sample.ui.sticky.Sticky5SampleActivity"/>
        <activity android:name="cz.kotlinwidget.ui.NewViewPagerListActivity"/>

        <activity android:name=".ui.layoutmanager.ui.SampleLinearLayoutManagerActivity"/>
        <activity android:name=".ui.layoutmanager.ui.WheelDateActivity"/>
        <activity android:name=".ui.layoutmanager.ui.GalleryActivity"/>
        <activity android:name=".ui.layoutmanager.ui.TableActivity"/>
    </application>

</manifest>
